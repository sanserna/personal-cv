{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "google",
    "prettier",
    "prettier/react",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier",
    "react",
    "jsx-a11y",
    "import",
    "graphql",
    "react-hooks"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react/forbid-prop-types": "off"
  }
}
